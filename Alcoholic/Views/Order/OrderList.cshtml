                                            </div>
                                            <button type="button" @@click="plus"
                                                class="col-2 p-0 btn btn-dark btn-sm plus-btn">
                                                <i class="fa fa-plus"></i>
                                            </button>
                                        </div>
                                        <button type="button" class="col-12 btn">加到購物車</button>
                                    </div>
                                </div>
                            </div>
                        </template>
                    </div>
                </div>
            </div>
            <!-- 推薦 -->
            <div class="tab-pane fade" id="recommendedCT" role="tabpanel" aria-labelledby="recommendedCT-tab"
                tabindex="0">
                <div class="container recommendedCT">
                    <div class="row g-4 mt-2">
                        <div id="carouselrecommendCaptions" class="carousel slide" data-bs-ride="carousel">
