@{
    ViewData["Title"] = "購物車";
    Layout = "_LayoutMobile";
}

<style>
    /*導覽列*/
    .nav-pills .nav-link.active,
    .nav-pills .show>.nav-link {
        --bs-nav-pills-link-active-bg: #333;
        border-bottom: #D8CDB9 1px solid;
        font-weight: bold;
        color: #D8CDB9;
        border-radius: 0;
    }
    .topnavbar {
        position: sticky;
        top: -1px;
        z-index: 1;
        background-color: rgba(22,22,20,0.8);
    }
    .nav-link:hover, .nav-link:focus{
        color: #D8CDB9;
    }
    /* 產品 */
    .signatureCT p,
    .classicCT p,
    .recommendedCT p,
    .customizedCT p {
        font-size: small;
        height: 40px;
    }

    .price {
        color: #333;
        text-decoration: line-through;
    }

    .discount {
        color: red;
        font-weight: bold;
    }

    .sale {
        background-color:rgba(200,0,0,0.8);
        font-weight: bold;
        float: right;
        color: #FFF;
        border-radius: 0 0 0.375rem 0;
    }

    .quantitygroup .btn{
        background-color: white;
        color: #D8CDB9;
        border: 0;
        border-radius: 0;
    }

    .quantitygroup .btn:hover {
            background-color: #C28C2E;
            color: white;
        }


    .qty_input {
        color: #333;
        border: 0;
    }

    .quantitygroup {
        border: #D8CDB9 solid 1px;
    }

    .btnaddtocart > .btn {
        background-color: #721B29;
        color: white;
        border: 0;
    }
        .btnaddtocart > .btn:hover {
            background-color: #D8CDB9;
            color: #333;
            border: 0;
    }

    /* 算命 */
    .fate {
        position: fixed;
        right: 0px;
        top: 20vh;
        width: 10vw;
        max-width: 100px;
        z-index: 2;
        display: inline;
    }

    /*model*/
    .modal-footer .btn {
        width: 100%;
        color:#FFF;
    }

        .modal-footer .btn:hover {
            color: #D8CDB9;
    }

    .modal-image {
        max-height: 280px;
    }

    .modal-spec {
        overflow-y: auto;
        max-height: 20vh;
    }

    .modal-bodyimg {
        text-align: center;
    }

    .modal-footer {
        background-color: #721B29;
    }

    /* 客製化 */
    .modal-content {
        background-color: #D8CDB9;
    }
    .modalinput {
        border: 0;
        background-color: transparent;
        font-size: 1rem;
        transition: ease;
        transition-duration: 0.3s;
    }

        .modalinput:hover{
            background-color: #B8AD89;
            font-size: 1.2rem;
            border-bottom: 1px solid #721B29;
            transition: ease;
            transition-duration: 0.3s;
        }
    .modal-header{
        border: 0;
    }

    .modal-headbackgroundcolor {
        background-color: #721B29;
        color: white;
    }
    /*購物車*/
    .shopCart{
        position:sticky;
        bottom:0;
        width:100%;
        display: flex;
        justify-content:center;
        align-content: center;
        background-color: #721B29;
        color: #FFF;
        height: 3rem;
    }
    .shopCart:hover{
            background-color: #D8CDB9;
            color: #333;
    }
    .shopCart i{
        display: flex;
        align-content:center;
        line-height: 3rem;
    }
    .shopCart h2{
        margin: auto 0;
        padding: 0 8px;
    }
</style>


